{
    "name": "crocodile2u/laravel-swagger-generator",
    "description": "Generate swagger spec for your REST API Laravel-based project",
    "type": "library",
    "require-dev": {
        "phpunit/phpunit": "^6.5"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Victor Bolshov",
            "email": "crocodile2u@gmail.com"
        }
    ],
    "require": {
        "crocodile2u/swagger-generator-integration": "*"
    },
    "autoload": {
        "psr-4": {
            "SwaggerGenerator\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\SwaggerGenerator\\": "./tests/"
        }
    },
    "repositories": {
        "swagger-generator-integration": {
            "type": "vcs",
            "url": "ssh://git@bitbucket.org/crocodile2u/swagger-generator-integration.git"
        }
    }
}
