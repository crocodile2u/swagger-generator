{
    "name": "nbwm/swagger-generator",
    "description": "Generate swagger spec for your REST API",
    "type": "library",
    "require-dev": {
        "phpunit/phpunit": "^6.5"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Victor Bolshov",
            "email": "victor.bolshov@nbwm.nl"
        }
    ],
    "require": {
        "nbwm/swagger-generator-integration": "*"
    },
    "autoload": {
        "psr-4": {
